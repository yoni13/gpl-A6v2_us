opengt
 set com 115200n81
 set comecho off
 set senddelay 0.05
 waitquiet 1 0.2

:start
let $x=$env("SETAPN")
let $y=$env("SETUSER")
let $z=$env("SETPASS")
let $a=$env("SETAUTH")
send "AT+CGDCONT=1,\"IP\",\""
send $x
send "\"^m"
 get 5 "" $s
 print $s
send "AT$QCPDPP=1,"
send $a
if $a="0" send "^m"
else send ",\"" send $z send "\",\"" send $y send "\"^m"
 get 5 "" $s
 print $s

:continue
 exit 0
