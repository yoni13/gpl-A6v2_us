Depends: +libc +USE_EGLIBC:librt +USE_EGLIBC:libpthread +libubox +PACKAGE_uhttpd-mod-tls:libubus +PACKAGE_uhttpd-mod-tls:libblobmsg-json +PACKAGE_uhttpd-mod-tls:libuci
