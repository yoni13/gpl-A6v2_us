@@
Config:
	menu "Configuration"
	depends on PACKAGE_kmod-fast-classifier
	config SFE_3G_SUPPORT
		bool "Sfe don't support ppp dial of 3g4gmobile to avoid kernel panic"
		default n

	endmenu
@@

Package: kmod-fast-classifier-noload
